{
    "name": "gazu1986/LaravelLombok",
    "description": "Laravel package for automatic getters and setters",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gazu1986\\LaravelLombok\\": "src/"
        }
    },
    "authors": [
        {
            "name": "gazu1986",
            "email": "vasic.marko.bg@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Gazu1986\\LaravelLombok\\LombokServiceProvider"
            ]
        }
    }
}